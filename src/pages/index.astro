import Scrollpart3 from "~/components/organisms/index/scrollpart3.astro";
import HumanMachine from "~/components/molecules/HumanMachine.astro";
import FooterParalaxScroll from "~/components/organisms/index/footerParalaxScroll.astro";
import LeftPara from "~/components/molecules/leftPara.astro";
import RightPara from "~/components/molecules/rightPara.astro";
import "src/styles/theme.css";


const { generator, site } = Astro;
const image = new URL("social.jpg", site);
const description = "Build fast websites, faster. Astro is a new kind of site builder for the modern web. Lightning-fast performance meets powerful developer experience.";
---

<!DOCTYPE html>


    <html lang="en" class="h-full motion-safe:scroll-smooth" data-theme="dark">
